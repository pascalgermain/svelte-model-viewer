{
	"name": "svelte-model-viewer",
	"version": "1.0.0",
	"description": "A Svelte model viewer.",
	"author": "Pascal Germain <pascalgermain@me.com>",
	"homepage": "https://pascalgermain.github.io/svelte-model-viewer",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "eslint --fix . && prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^1.11.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.22.4",
		"@tailwindcss/forms": "^0.5.4",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.27",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"sass": "^1.64.2",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.6",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
	},
	"type": "module"
}
